{
  "expo": {
    "name": "pawstagram",
    "slug": "pawstagram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "scheme": "pawstagram",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "output": "server"
    },
    "plugins": [
      "expo-font",
      [
        "expo-router",
        {
          "origin": "https://evanbacon.dev/"
        }
      ]
    ]
  }
}
